type User {
  id: ID!
}

type Deck {
  id: ID!
  title: String
  dueCards: [Card]
}

type Card {
  id: ID!
  front: String
  back: String
}

type Query {
  user: User!
  deck(id: String!): Deck
  card(deckId: ID!, id: ID!): Card
}

